rm ~/.lesshst; touch $_
rm ~/.bash_history; touch $_
rm ~/.zsh_history; touch $_
rm ~/.python_history; touch $_
rm ~/.mysql_history; touch $_
rm ~/.config/nvim/.netrwhist; touch $_
rm ~/.octave_hist; touch $_
rm ~/.config/ranger/history; touch $_
rm ~/.viminfo; touch $_
rm ~/.psql_history; touch $_
