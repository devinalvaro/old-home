#!/bin/bash

rm -f ~/.local/share/nvim/shada/*
rm -f ~/.vim/fzf/*
rm -f ~/.vim/undo/*

rm ~/.bash_history; touch $_
rm ~/.lesshst; touch $_
rm ~/.local/share/nvim/log; touch $_
rm ~/.mysql_history; touch $_
rm ~/.octave_hist; touch $_
rm ~/.psql_history; touch $_
rm ~/.python_history; touch $_
rm ~/.viminfo; touch $_
rm ~/.vim/.netrwhist; touch $_
rm ~/.zsh_history; touch $_
