[core]
	ignorecase = false

[credential]
	helper = cache

[push]
	default = simple

[user]
	email = devin.alvaro@gmail.com
	name = Devin Alvaro

[alias]
	a  = add
	aa = add --all
	ap = add --patch

	br  = branch
	bd  = branch --delete
	bm  = branch --merged
	bdm = "!git branch --merged | grep  -v '\\*\\|master' | xargs -n 1 git branch -d"

	cm = commit
	ca = commit --amend
	ce = commit --allow-empty-message

	ch = cherry
	cp = cherry-pick

	df = diff
	dc = diff --cached

	lg = log
	lo = log --oneline

	mr  = merge
	mra = merge --abort

	rb  = rebase
	rba = rebase --abort
	rbc = rebase --continue
	rbs = rebase --skip

	sh = stash
	shk = stash --keep-index

	st = status --short
	sa = status --short -uall

	cl = clone
	cn = clean
	co = checkout
	fe = fetch
	mv = mv
	p  = push
	pl = pull
	re = remote
	rl = reflog
	rm = rm
	rs = reset
	rv = revert

[url "ssh://gitlab@git.garena.com:2222/"]
	insteadOf = https://git.garena.com/
